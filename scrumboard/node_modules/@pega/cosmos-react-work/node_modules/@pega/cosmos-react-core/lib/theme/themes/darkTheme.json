{
    "base": {
        "palette": {
            "app-background": "#222222",
            "primary-background": "#333333",
            "secondary-background": "#666666",
            "foreground-color": "white",
            "brand-primary": "#003a66",
            "urgent": "#f66f78",
            "warn": "#ffaa75",
            "success": "#7ee791",
            "interactive": "#71c1ff",
            "pending": "#d6bcf5",
            "border-line": "#939393",
            "skeleton": "#939393",
            "scrollbar-track": "rgba(0, 0, 0, 0.2)",
            "scrollbar-thumb": "rgba(255, 255, 255, 0.6)",
            "scrollbar-thumb-hover": "rgba(255, 255, 255, 0.7)",
            "light": "#000000",
            "dark": "#ffffff"
        },
        "shadow": {
            "focus": "0 0 0.25rem rgba(255, 255, 255, 0.95)"
        }
    },
    "components": {
        "form-control": {
            ":hover": {
                "border-color": "#fafafa"
            },
            ":disabled": {
                "background-color": "#676767"
            },
            ":read-only": {
                "background-color": "#676767"
            }
        },
        "tooltip": {
            "foreground-color": "#3f3f3f",
            "background-color": "#fafafa"
        },
        "badges": {
            "keyboard": {
                "background-color": "#676767",
                "border-color": "#939393"
            }
        }
    }
}
